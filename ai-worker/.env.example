# --- APP CONFIG ---
PORT=8000
ENVIRONMENT=development

# --- CLOUDINARY (Để upload ảnh trong suốt lên) ---
# Copy y hệt bên Server Node.js
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# --- REDIS (Nếu worker connect trực tiếp vào Queue) ---
REDIS_URL=redis://default:your_password@your-instance.upstash.io:6379

# --- AI MODELS CACHE ---
# Để tránh tải lại model mỗi lần chạy, cache vào thư mục local
U2NET_HOME=./.u2net
